      g#outer-text
         - var fontSize = 200
         - var padding = 200
         - var offsetY=0
         - var fontFamily='Helvetica Neue'
         g#upper-text
            - var radius = sourceWidth/2 + padding
            - var margin = (width /2)-radius
            path#upper-text-path(
               d=`M ${margin},${height/2} A ${radius},${radius} 0 1 1 ${width - margin},${height/2}`
               fill="none"
               stroke="none"
            )
            text#text1(style=`font-weight:bold;font-size:${fontSize}px;line-height:1;font-family:'${fontFamily}';fill:${outerTextFill}`)
               textPath#text-path-lsv(
                  startOffset="50%"
                  text-anchor="middle"
                  style="letter-spacing:-10px"
                  href="#upper-text-path"
               )
                  | SOL LUCET OMNIBUS
         g#lower-text
            - var lowerPadding=padding-(fontSize/4)
            - var radius = sourceWidth/2 + lowerPadding + fontSize
            - var margin = (width /2)-radius
            path#lower-text-path(
               d=`m ${width-margin},${height/2} A ${radius},${radius} 0 0 1 ${margin},${height/2}`
               fill="none"
               stroke="none"
            )
            text#text2(style=`font-weight:bold;font-size:${fontSize}px;line-height:1;font-family:'${fontFamily}';fill:${outerTextFill}`)
               textPath#text-path-place(
                  startOffset="50%"
                  side="right"
                  text-anchor="middle"
                  style="letter-spacing:4px"
                  href="#lower-text-path"
               )
                  | NOS LUCET BIBENDUM

